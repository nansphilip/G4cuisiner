{
    "root": true,
    "plugins": ["tailwindcss"],
    "extends": ["next/core-web-vitals", "next/typescript", "plugin:tailwindcss/recommended"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js"],
            "parser": "@typescript-eslint/parser"
        }
    ],
    "settings": {
        "callees": ["classnames", "combo"]
    }
}
